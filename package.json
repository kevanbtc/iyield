  "description": "iYield Protocol - Insurance Cash Surrender Value Tokenization Platform",
  "main": "index.js",
  "scripts": {
    "install-all": "npm run install-contracts && npm run install-frontend",
    "install-contracts": "cd contracts && npm install",
